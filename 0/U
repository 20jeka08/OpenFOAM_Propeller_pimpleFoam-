FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{

    "(Inlet|Outlet|External)"
    {
        type            inletOutlet;
        value           $internalField;
        inletValue      uniform (0 0 0);
    }

    Wall
    {
        type            noSlip;
    }



    "(AMI_1|AMI_2)"
    {
        type            cyclicAMI;
    }
}

// ************************************************************************* //
